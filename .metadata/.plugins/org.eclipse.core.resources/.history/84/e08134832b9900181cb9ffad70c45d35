package com.loops;

import java.util.Scanner;

public class DemoSwitch {

	public static void main(String[] args) {
		
		System.out.println("Enter a character:");
		Scanner scan = new Scanner(System.in);
		String checkString = scan.nextLine();
		
		switch(checkString.toUpperCase()) {
		case "A": 
		case "E":
		case "I":
		case "O":
		case "U": System.out.println("Your character is a vowel");
			      break;
		default: System.out.println("Your character is a consonant");
				 break;
		}
		

	}

}
