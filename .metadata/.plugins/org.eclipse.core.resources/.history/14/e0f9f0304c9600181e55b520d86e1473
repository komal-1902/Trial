
public class MathFunctions {

	public static void main(String[] args) {
		
		double num1 = 4.2;
		double num2 = 5.8;
		
		System.out.println("1st Number: " + num1 + "\t2nd Number: " + num2);
		
		System.out.println("Ceil of 1st number: " + Math.ceil(num1));
		System.out.println("Maximum of two numbers: " + Math.max(num1, num2));
		System.out.println("Square of 1st number: " + Math.sqrt(num1));
		System.out.println("Rounding 1st number: " + Math.round(num1));

	}

}
