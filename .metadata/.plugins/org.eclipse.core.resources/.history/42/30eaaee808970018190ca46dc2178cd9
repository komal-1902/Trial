import java.util.Arrays;

public class DemoArrayOperations {

	public static void main(String[] args) {
		
		int[] numbers = {123, 43, 15, 76, 86, 43, 90, 24, 14, 64};
		
		//Arrays.fill(numbers, 10);
		//Arrays.fill(numbers,  1, 3, 5);
		
		int[] numbers_copy = Arrays.copyOf(numbers, 11);
		int[] numbers_copy_range = Arrays.copyOfRange(numbers, 2, 5);
		
		for(int num : numbers_copy) {
			System.out.print(num + " ");
		}

	}

}
