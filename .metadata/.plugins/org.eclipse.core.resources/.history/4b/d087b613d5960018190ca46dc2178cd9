
public class Demo1D {

	public static void main(String[] args) {

		int[] numbers;
		
		numbers = new int[5];
		numbers[0] = 2;
		numbers[1] = 4;
		numbers[2] = 2;
		numbers[3] = 5;
		numbers[4] = 6;
		
		int sumOfNumbers = numbers[0] + numbers[1] + numbers[2] + numbers[3] + numbers[4];
		
		System.out.println("Array: " + numbers[0] + " " + numbers[1] + " " + numbers[2] + " " + numbers[3] + " " + numbers[4]);
		System.out.println("Sum of numbers of the array: " + sumOfNumbers);

	}

}
