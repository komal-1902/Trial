
public class DemoStatic {
	
	private float age;
	private String name;
	
	private static int value; 
	
	public DemoStatic () {
		age = 12.12f;
		name = "ABCD";
	}
	
	static {
		value = 10;
	}
	
	public void display() {
		System.out.println(name + "\t" + age + "\t" + value);
	}
	
	public static void show() {
		System.out.println(value);
	}
	
	public static void main(String args[]) {
		DemoStatic.show();
	}

}
