package com.pojo;

import java.util.Arrays;

public class Student {
	
	private String name;
	private int[] marks;
	private int rollNo;
	
	public Student() {
		name = "Komal";
		rollNo = 37;
		Arrays.fill(marks, 90);
	}
	
	public Student(String name, int[] marks, int rollNo) {
		super();
		this.name = name;
		this.marks = marks;
		this.rollNo = rollNo;
	}
	
	

}
