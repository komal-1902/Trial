
public class DemoLoop {

	public static void main(String[] args) {
		
		int[] array = {12, 34, 44, 54, 34, 55, 65, 78, 90, 23, 90};
		
		int i = 0;
		while(i < array.length) {
			System.out.print(array[i++] + " ");
		}
		System.out.println();
		
		do {
			System.out.print(array[i++] + " ");
		}while(i < array.length);

	}

}
